---
import DefaultLayout from '../layouts/DefaultLayout.astro'
---

<DefaultLayout title="404">
  <section class="my-12">
    <div class="container">
      <h1>404 - Page Not Found</h1>
    </div>
  </section>
  <section class="my-12">
    <div class="space-content container">
      <p class="text-2xl">The webpage your are accessing does not exist. You can naviagate back to the homepage, or reach out to us diredtly at <a href="mailto:contact@zklocus.dev">contact@zklocus.dev</a></p>
      <a class="button" href="/">Back To Homepage 📍</a>
      <a class="button" href="/contact">Contact Us 📱</a>
    </div>
  </section>
</DefaultLayout>
